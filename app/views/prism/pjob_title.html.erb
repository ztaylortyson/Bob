<h1>Prism#pjob_title</h1>
<%= params.inspect %>
<hr>
<% count = 1 %>
<table class="mytables">

	<tr>
		<thead>
		<th>Count</th>
		<th>Member ID</th>
		<th>Member</th>
		<th>Job Title</th>
		<th>Contract?</th>
		<th>Date Hired</th>
		<th>Discharge</th>
		<th>Weeks Worked</th>
	</thead>
	</tr>
	<% mwc = 0 %>
	<% @members.each do |m| %>
	<tr>
		<td><%= count %></td>
		<td><%= m.id %></td>
		<td><%= link_to "#{m.lname}, #{m.fname}", member_path(m), class: "butt round" %></td>
		<td><%= m.job_title %></td>
		<td><% if m.contracts.first %>Yes<% mwc += 1 %><% end %></td>

		<td><%= m.doh %></td>
		<td><%= m.dod %></td>
		<td><%= weeks(m) %></td>
	</tr>
	<% count += 1 %>
	<% end %> 
</table>
<p>Members with Contracts: <%= mwc %></p>



